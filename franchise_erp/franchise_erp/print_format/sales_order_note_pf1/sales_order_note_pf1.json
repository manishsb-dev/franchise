 "css": "/* 1. Universal Reset */\n* {\n    margin: 0 !important;\n    padding: 0 !important;\n    box-sizing: border-box !important;\n    letter-spacing: 0px !important;\n    font-family: Arial, Helvetica, sans-serif !important;\n}\n\n/*td{*/\n/*    border:0px !important;*/\n/*}*/\n\n/* 2. Grid System */\n.table-content {\n    display: table !important;\n    width: 100%;\n}\n\n.row {\n    display: table-row-group;\n    width: 100%;\n}\n\n.col-50 {\n    display: table-cell;\n    max-width: 50% !important;\n    width: 50% !important;\n    vertical-align: middle;\n}\n\n.col-100 {\n    display: table-cell;\n    max-width: 100% !important;\n    width: 100%;\n}\n\n/* 3. Header & Logo */\n.sb-header {\n    width: 100%;\n    display: block;\n}\n\n.sb-header .header-content {\n    padding: 0.35rem 1.8rem !important;\n}\n\n.sb-header .header-content .header-logo {\n    width: 150px !important;\n    vertical-align: top !important;\n    margin-bottom: 5px !important; \n}\n\n/* 4. Font Sizes */\n.fs-mini, .fs-mini * { font-size: 10px !important; }\n.fs-xsmall, .fs-xsmall * { font-size: 10px !important; }\n.fs-small, .fs-small * { font-size: 12px !important; }\n.fs-large { font-size: 16px !important; }\n\n/* 5. Table Formatting */\ntable {\n    width: 100% !important;\n    border-collapse: collapse;\n}\n\n.table th, .table td {\n    padding: 5px 5px !important;\n    border: 1px solid rgb(184, 184, 184);\n}\n\n.table-p-0 th, .table-p-0 td { padding: 0px !important; }\n.table-p-1 th, .table-p-1 td { padding: 4px 4px !important; }\n.table-p-2 th, .table-p-2 td { padding: 4px 5px !important; }\n\n.fw-bold { font-weight: bold !important; }\n.fw-bolder { font-weight: bolder !important; }\n.text-center { text-align: center !important; }\n.text-right, .text-end { text-align: right !important; }\n\n.text-muted { color: #000 !important; } \n\n.fst-upper { text-transform: uppercase !important; }\n.fst-capitalize { text-transform: capitalize !important; }\n\n/* 6. Utility Classes - UPDATED */\n.d-flex { \n    display: flex !important; \n    flex-direction: row !important; \n    flex-wrap: nowrap !important;  \n}\n\n.justify-content-between { \n    justify-content: space-between !important; \n    width: 100% !important; \n}\n\n/* 7. Borders & Backgrounds */\n.border-top { border-top: 1px solid rgb(184, 184, 184) !important; }\n.border-default { border-color: rgb(184, 184, 184) !important; }\n.border-none-all, .border-none-all * { border: none !important; }\n\n.bg-light {\n    background-color: rgb(242, 242, 242) !important;\n    -webkit-print-color-adjust: exact !important;\n}\n\n/* 8. Print Settings */\n@page { margin: 5mm; }\n.content { padding: 10px !important; }\n\n.page-number:after {\n    counter-increment: page;\n    content: counter(page);\n}\n\n.print-color-exact {\n    -webkit-print-color-adjust: exact !important;\n}\n\n.d-inline-flex { \n    display: inline-flex !important; \n    align-items: center !important; \n    white-space: nowrap !important;  \n}",
 "custom_format": 1,
 "default_print_language": "en",
 "disabled": 0,
 "doc_type": "Sales Order",
 "docstatus": 0,
 "doctype": "Print Format",
 "font_size": 14,
 "html": "{% set cgst_rate = [] -%}\n{% set sgst_rate = [] -%}\n{% set igst_rate = [] -%}\n{% set cgst_account_head = [] -%}\n{% set sgst_account_head = [] -%}\n{% set igst_account_head = [] -%}\n{% set grand_total = {\"qty\":0.0, \"amount\":0.0, \"cgst_amt\":0.0, \"sgst_amt\":0.0, \"igst_amt\":0.0} %}\n\n<!-- Extract Tax Rates from Taxes Table -->\n{%- for tax in doc.taxes -%}\n    {% if 'CGST' in tax.account_head -%}\n        {% set _ = cgst_account_head.append(tax.account_head) %}\n        {% set _ = cgst_rate.append(tax.rate) %}\n    {%- endif -%}\n    {% if 'SGST' in tax.account_head -%}\n        {% set _ = sgst_account_head.append(tax.account_head) %}\n        {% set _ = sgst_rate.append(tax.rate) %}\n    {%- endif -%}\n    {% if 'IGST' in tax.account_head -%}\n        {% set _ = igst_account_head.append(tax.account_head) %}\n        {% set _ = igst_rate.append(tax.rate) %}\n    {%- endif -%}\n{%- endfor -%}\n\n{%- with \n    extra_data = {\"is_igst\":False, \"taxable_amt\":0}, \n    total_data = {'basic_amt':0, 'qty':0, 'taxable_amt':0, 'total_tax': 0, 'total_amt':0} \n-%}\n\n<section class=\"content\">\n  <table class=\"table table-p-0\">\n<thead>\n  <!-- Fetch Company Details -->\n  {% set company_doc = frappe.get_doc(\"Company\", doc.company) %}\n  \n  <!-- Header -->\n  <tr>\n    <td colspan=\"10\">\n      <div class=\"table-content mb-0 pb-1\">\n        <div class=\"row\">\n          <div class=\"col-100\">\n            <div class=\"sb-header border-top border-default\">\n              <div class=\"header-content\">\n                <div class=\"table-content\">\n                  <div class=\"row\">\n                    <!-- Logo Section -->\n                    <div class=\"col-50\">\n                      <img src=\"{{ company_doc.logo_for_printing or company_doc.company_logo }}\" class=\"header-logo\" style=\"max-height: 75px;\">\n                      <!--<p class=\"fs-mini fw-bold mt-1\">\"The Best In You...\"</p>-->\n                    </div>\n                    \n                    <!-- Text Details Section -->\n                    <div class=\"col-50\">\n                      <table class=\"table border-none-all table-p-1\">\n                        <tbody>\n                          <tr>\n                            <td colspan=\"2\">\n                              <p class=\"fs-small fw-bolder fst-upper\">\n                                {{ doc.company }}\n                              </p>\n                            </td>\n                          </tr>\n                          <tr>\n                            <td style=\"width: 60px !important;\">\n                              <p class=\"fs-small text-muted fw-bold\">GSTIN:</p>\n                            </td>\n                            <td>\n                              <p class=\"fs-small text-muted fw-bold\">{{ doc.company_gstin or company_doc.gstin or \"-\" }}</p>\n                            </td>\n                          </tr>\n                          <tr>\n                            <td style=\"width: 60px !important;\">\n                              <p class=\"fs-small text-muted fw-bold\">PAN NO. </p>\n                            </td>\n                            <td>\n                              <p class=\"fs-small text-muted fw-bold\">{{ company_doc.pan or \"-\" }}</p>\n                            </td>\n                          </tr>\n                          \n                          <!-- FIXED: Address Logic (Using company_address instead of billing_address) -->\n                          <tr>\n                            <td style=\"vertical-align: top;\">\n                              <p class=\"fs-small text-muted\">Address:</p>\n                            </td>\n                            <td>\n                              <div class=\"fs-small text-muted\">\n                                {% if doc.company_address %}\n                                  {% set addr = frappe.get_doc(\"Address\", doc.company_address) %}\n                                  \n                                  <!-- Line 1 -->\n                                  {{ addr.address_line1 }}\n                                  \n                                  <!-- Line 2 -->\n                                  {% if addr.address_line2 %}\n                                    {{ addr.address_line2 }}\n                                  {% endif %}\n                                  \n                                  <!-- City and State only -->\n                                  {{ addr.city }}{% if addr.state %}, {{ addr.state }}{% endif %}\n                                {% else %}\n                                  <!-- Fallback to pre-rendered display if doc.company_address is missing -->\n                                  {{ doc.company_address_display }}\n                                {% endif %}\n                              </div>\n                            </td>\n                          </tr>\n                        </tbody>\n                      </table>\n\n                      <!-- Contact Info (Phone & Email from Company Doc) -->\n                      <div class=\"d-flex justify-content-between\">\n                        <span class=\"d-inline-flex align-items-center\">\n                          <span style=\"color: #230bb3 !important; font-size: 18px !important; margin-right: 4px;\" class=\"print-color-exact\">\n                            &#9990;\n                          </span>\n                          <p class=\"d-inline fs-mini text-muted font_no\">{{ company_doc.phone_no or \"\" }}</p>\n                        </span>\n                        \n                        <span class=\"d-inline-flex align-items-center\">\n                          <span style=\"color: #230bb3 !important; font-size: 18px !important; margin-right: 4px;\" class=\"print-color-exact\">\n                            &#9993;\n                          </span>\n                          <p class=\"d-inline fs-mini text-muted font_no\">{{ company_doc.email or \"\" }}</p>\n                        </span>\n                      </div>\n                      \n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </td>\n  </tr>\n</thead>\n    <tbody>\n      <!-- TITLE BAR -->\n      <tr><td colspan=\"10\"><div class=\"py-1\"><p class=\"fs-large fw-bold text-center\">Sales Order Note</p></div></td></tr>\n\n      <!-- MIDDLE PART: CUSTOMER INFO (MAPPED TO SALES ORDER) -->\n     <!-- MIDDLE PART: CUSTOMER INFO (FIXED ADDRESS, PHONE & EMAIL) -->\n{% set billing_addr = frappe.get_doc(\"Address\", doc.customer_address) if doc.customer_address else None %}\n{% set shipping_addr = frappe.get_doc(\"Address\", doc.shipping_address_name) if doc.shipping_address_name else None %}\n\n<tr>\n  <td colspan=\"10\" style=\"padding-top: 0px !important;\">\n    <table class=\"table border-none-all table-p-1\" style=\"width: 100%;\">\n      <tbody>\n        <tr>\n          <!-- LEFT COLUMN: Bill To -->\n          <td style=\"width: 50%; vertical-align: top; border-right: 1px solid #b8b8b8 !important; padding-right: 5px;\">\n            <table class=\"table border-none-all table-p-0\" style=\"width: 100%;\">\n              <tr>\n                <td style=\"width: 110px;\"><p class=\"fs-small fw-bold\">Bill To</p></td>\n                <td style=\"width: 10px;\"><p class=\"fs-small fw-bold\">:</p></td>\n                <td><p class=\"fs-small fw-bold\">{{ doc.customer_name }}</p></td>\n              </tr>\n              <tr>\n                <td style=\"vertical-align: top;\"><p class=\"fs-small\">Billing Address</p></td>\n                <td><p class=\"fs-small\">:</p></td>\n                <td>\n                  <p class=\"fs-small\">\n                    {% if billing_addr %}\n                      {{ billing_addr.address_line1 }}\n                      {% if billing_addr.address_line2 %}, {{ billing_addr.address_line2 }}{% endif %}<br>\n                      {{ billing_addr.city }}{% if billing_addr.state %}, {{ billing_addr.state }}{% endif %}\n                      {% if billing_addr.pincode %}, {{ billing_addr.pincode }}{% endif %}\n                    {% else %}\n                      {{ doc.address_display|striptags }}\n                    {% endif %}\n                  </p>\n                </td>\n              </tr>\n              <tr>\n                <td><p class=\"fs-small\">Contact No</p></td>\n                <td><p class=\"fs-small\">:</p></td>\n                <td><p class=\"fs-small\">{{ doc.contact_mobile or doc.contact_phone or (billing_addr.phone if billing_addr else \"-\") }}</p></td>\n              </tr>\n              <tr>\n                <td><p class=\"fs-small\">Contact Person</p></td>\n                <td><p class=\"fs-small\">:</p></td>\n                <td><p class=\"fs-small\">{{ doc.contact_display or \"-\" }}</p></td>\n              </tr>\n              <tr>\n                <td><p class=\"fs-small\">Email</p></td>\n                <td><p class=\"fs-small\">:</p></td>\n                <td><p class=\"fs-small\">{{ doc.contact_email or (billing_addr.email_id if billing_addr else \"-\") }}</p></td>\n              </tr>\n              <tr>\n                <td><p class=\"fs-small\">GSTIN</p></td>\n                <td><p class=\"fs-small\">:</p></td>\n                <td><p class=\"fs-small\">{{ doc.billing_address_gstin or \"-\" }}</p></td>\n              </tr>\n              <tr>\n                <td><p class=\"fs-small\">GST State</p></td>\n                <td><p class=\"fs-small\">:</p></td>\n                <td><p class=\"fs-small\">{{ doc.place_of_supply or \"Delhi(07)\" }}</p></td>\n              </tr>\n              <!-- LEFT COLUMN: Bill To Details -->\n<tr>\n  <td style=\"width: 110px;\"><p class=\"fs-small fw-bold\">Outstanding Amt</p></td>\n  <td style=\"width: 10px;\"><p class=\"fs-small fw-bold\">:</p></td>\n  <td>\n    <p class=\"fs-small\">\n      {% set outstanding_query = frappe.db.sql(\"SELECT SUM(debit) - SUM(credit) FROM `tabGL Entry` WHERE party_type='Customer' AND party=%s\", doc.customer) %}\n      {% set total_outstanding = outstanding_query[0][0] if outstanding_query and outstanding_query[0][0] else 0.0 %}\n      \n      {{ frappe.utils.fmt_money(total_outstanding, currency=doc.currency) }}\n    </p>\n  </td>\n</tr>\n            </table>\n          </td>\n\n          <!-- RIGHT COLUMN: Ship To & Order Info -->\n          <td style=\"width: 50%; vertical-align: top; padding-left: 5px;\">\n            <table class=\"table border-none-all table-p-0\" style=\"width: 100%;\">\n              <tr>\n                <td style=\"width: 110px;\"><p class=\"fs-small fw-bold\">Ship To</p></td>\n                <td style=\"width: 10px;\"><p class=\"fs-small fw-bold\">:</p></td>\n                <td><p class=\"fs-small fw-bold\">{{ doc.customer_name }}</p></td>\n              </tr>\n              <tr>\n                <td style=\"vertical-align: top;\"><p class=\"fs-small\">Shipping Address</p></td>\n                <td><p class=\"fs-small\">:</p></td>\n                <td>\n                  <p class=\"fs-small\">\n                    {% if shipping_addr %}\n                      {{ shipping_addr.address_line1 }}\n                      {% if shipping_addr.address_line2 %}, {{ shipping_addr.address_line2 }}{% endif %}<br>\n                      {{ shipping_addr.city }}{% if shipping_addr.state %}, {{ shipping_addr.state }}{% endif %}\n                      {% if shipping_addr.pincode %}, {{ shipping_addr.pincode }}{% endif %}\n                    {% elif billing_addr %}\n                      {{ billing_addr.address_line1 }}\n                      {% if billing_addr.address_line2 %}, {{ billing_addr.address_line2 }}{% endif %}<br>\n                      {{ billing_addr.city }}, {{ billing_addr.state }}\n                    {% else %}\n                      -\n                    {% endif %}\n                  </p>\n                </td>\n              </tr>\n              <tr>\n                <td><p class=\"fs-small\">Site GSTIN</p></td>\n                <td><p class=\"fs-small\">:</p></td>\n                <td><p class=\"fs-small\">{{ doc.billing_address_gstin or \"-\" }}</p></td>\n              </tr>\n              <tr>\n                <td><p class=\"fs-small\">Site GST State</p></td>\n                <td><p class=\"fs-small\">:</p></td>\n                <td><p class=\"fs-small\">{{ doc.place_of_supply or \"Delhi(07)\" }}</p></td>\n              </tr>\n              <tr>\n                <td><p class=\"fs-small\">Agent Name</p></td>\n                <td><p class=\"fs-small\">:</p></td>\n                <td><p class=\"fs-small\">{{ doc.custom_agent or \"-\" }}</p></td>\n              </tr>\n              <tr>\n                <td><p class=\"fs-small\">Transporter Name</p></td>\n                <td><p class=\"fs-small\">:</p></td>\n                <td><p class=\"fs-small\">{{ doc.custom_transporter or \"-\" }}</p></td>\n              </tr>\n              <tr>\n                <td><p class=\"fs-small fw-bold\">SO No</p></td>\n                <td><p class=\"fs-small fw-bold\">:</p></td>\n                <td><p class=\"fs-small fw-bold\">{{ doc.name }}</p></td>\n              </tr>\n              <tr>\n                <td><p class=\"fs-small fw-bold\">SO Date</p></td>\n                <td><p class=\"fs-small fw-bold\">:</p></td>\n                <td><p class=\"fs-small fw-bold\">{{ doc.get_formatted(\"transaction_date\") }}</p></td>\n              </tr>\n            </table>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n  </td>\n</tr>\n\n      <!-- BANK INFO BAR (STYLING KE LIYE EK EXTRA ROW) -->\n      <!--<tr><td colspan=\"10\" class=\"border py-1 bg-light px-2\" style=\"background-color: #f9f9f9;\"><p class=\"fs-mini mb-0 fw-bold\">ICICI BANK, Chandni Chowk Branch A/C NO. 777705500334, RTGS/NEFT Code - ICIC0006292.</p></td></tr>-->\n\n      <!-- MIDDLE PART: ITEMS TABLE (SALES ORDER MAPPING) -->\n      <tr >\n        <td colspan=\"10\" style=\"    border-bottom: 0px;\">\n          <table class=\"table table-p-2\" style=\"border-collapse: collapse; width:100%;\">\n            <thead>\n              <tr class=\"text-center bg-light\">\n                <td style=\"width: 10%;    border-left: 0px;\"><p class=\"fs-xsmall fw-bold\">Barcode</p></td>\n                <td style=\"width: 25%;\"><p class=\"fs-xsmall fw-bold\">Particulars</p></td>\n                <td style=\"width: 10%;\"><p class=\"fs-xsmall fw-bold\">HSN Code</p></td>\n                <td style=\"width: 5%;\"><p class=\"fs-xsmall fw-bold\">Qty</p></td>\n                <td style=\"width: 10%;\"><p class=\"fs-xsmall fw-bold\">MRP</p></td>\n                <td style=\"width: 10%;\"><p class=\"fs-xsmall fw-bold\">Rate <br><small>(Incl GST)</small></p></td>\n                <td style=\"width: 10%;\"><p class=\"fs-xsmall fw-bold\">Rate <br><small>(Pre GST)</small></p></td>\n                <td style=\"width: 10%;    border-right: 0px;\"><p class=\"fs-xsmall fw-bold text-end\">Amount</p></td>\n              </tr>\n            </thead>\n            <tbody>\n              {%- for item in doc.items -%}\n                {% set row_tax_amt = 0 %}\n                {%- for tax in doc.taxes -%}\n                    {% set tax_details = json.loads(tax.item_wise_tax_detail) if tax.item_wise_tax_detail else {} %}\n                    {% set item_key = item.item_code if item.item_code in tax_details else item.name %}\n                    {% if item_key in tax_details %}{% set row_tax_amt = row_tax_amt + tax_details[item_key][1] %}{% endif %}\n                {%- endfor -%}\n                {% set rate_incl_gst = (item.net_amount + row_tax_amt) / item.qty if item.qty > 0 else 0 %}\n                <tr>\n                  <td style=\"    border-left: 0px !important;\" class=\"fs-mini text-center border\" style=\"border: 1px solid #b8b8b8;\">{{ item.barcode or \"-\" }}</td>\n                  <td class=\"fs-mini border\" style=\"border: 1px solid #b8b8b8;\"><strong>{{ item.item_name }}</strong></td>\n                  <td class=\"fs-mini text-center border\" style=\"border: 1px solid #b8b8b8;\">{{ item.gst_hsn_code or \"-\" }}</td>\n                  <td class=\"fs-mini text-center border\" style=\"border: 1px solid #b8b8b8;\">{{ item.qty }}</td>\n                  <td class=\"fs-mini text-end border\" style=\"border: 1px solid #b8b8b8;\">{{ frappe.utils.fmt_money(item.mrp or 0, currency=doc.currency) }}</td>\n                  <td class=\"fs-mini text-end border\" style=\"border: 1px solid #b8b8b8;\">{{ frappe.utils.fmt_money(rate_incl_gst, currency=doc.currency) }}</td>\n                  <td class=\"fs-mini text-end border\" style=\"border: 1px solid #b8b8b8;\">{{ frappe.utils.fmt_money(item.net_rate, currency=doc.currency) }}</td>\n                  <td style=\"    border-right: 0px !important;\" class=\"fs-mini text-end fw-bold border\" style=\"border: 1px solid #b8b8b8;\">{{ frappe.utils.fmt_money(item.net_amount, currency=doc.currency) }}</td>\n                </tr>\n                {%- set _ = total_data.update({'qty': total_data.qty + item.qty, 'taxable_amt': total_data.taxable_amt + item.net_amount}) -%}\n              {%- endfor -%}\n              <tr class=\"bg-light\">\n                <td colspan=\"3\" class=\"fs-mini fw-bold text-end border\" style=\"    border-left: 0px !important;\"> Total</td>\n                <td class=\"fs-mini fw-bold text-center border\" style=\"border: 1px solid #b8b8b8;\">{{ total_data.qty }}</td>\n                <td colspan=\"3\" class=\"border\" style=\"border: 1px solid #b8b8b8;\"></td>\n                <td class=\"fs-mini fw-bold text-end border\" style=\"border-right: 0px !important;\" >{{ frappe.utils.fmt_money(total_data.taxable_amt, currency=doc.currency) }}</td>\n              </tr>\n            </tbody>\n          </table>\n        </td>\n      </tr>\n\n      <!-- TOTALS & REMARKS (KEEPING YOUR EXACT STRUCTURE) -->\n      <tr>\n        <td colspan=\"10\" style=\"padding: 0 !important;border-top:0px;border-bottom:0px;\">\n          <table class=\"table table-p-1\" style=\"width: 100%; border-collapse: collapse; margin-bottom: 0px;\">\n            <tbody>\n              <tr>\n                <td style=\"width: 48%; vertical-align: top !important; border-right: 1px solid #b8b8b8;     border-bottom: 0px !important ;border-top: 0px !important;border-left:0px !important; padding: 2px 10px !important;\">\n                    <div class=\"fs-small fw-bold\" style=\"text-decoration: underline !important;\">Terms & Conditions / Remarks:</div>\n                    <div class=\"fs-mini terms-content\">{{ doc.terms or 'As per company policy.' }}</div>\n                </td>\n                <td style=\"width: 38.4%; padding: 0 !important; vertical-align: top; border-bottom: 0px !important ;border-top: 0px !important;border-left:0px !important;border-right:0px !important;\">\n                  <table class=\"table border-none-all\" style=\"width: 100%;\">\n                    <tbody>\n                        <tr><td style=\"width: 75.2%; border-right: 1px solid #b8b8b8 !important; border-bottom: 1px solid #e2e2e2 !important; padding: 5px;\"><p class=\"fs-mini\">Basic Amount</p></td><td style=\"padding: 5px; text-align: right;\"><p class=\"fs-mini fw-bold\">{{ frappe.utils.fmt_money(doc.total, currency=doc.currency) }}</p></td></tr>\n                        {% for tax in doc.taxes %}{% if tax.tax_amount != 0 %}\n                        <tr><td style=\"width: 75.2%; border-right: 1px solid #b8b8b8 !important; border-bottom: 1px solid #e2e2e2 !important; padding: 5px;\"><p class=\"fs-mini\">{{ tax.description }}</p></td><td style=\"padding: 5px; text-align: right;\"><p class=\"fs-mini\">+ {{ tax.get_formatted(\"tax_amount\") }}</p></td></tr>\n                        {% endif %}{% endfor %}\n                        <tr style=\"background-color: #f0f0f0;\"><td style=\"width: 75.2%; border-right: 1px solid #b8b8b8 !important; padding: 7px 5px;\"><p class=\"fs-mini fw-bold\">Grand Total</p></td><td style=\"padding: 7px 5px; text-align: right;\"><p class=\"fs-small fw-bold\">{{ doc.get_formatted(\"grand_total\") }}</p></td></tr>\n                        <tr><td colspan=\"2\" style=\"padding: 5px;\"><p class=\"fs-mini\"><span class=\"fw-bold\">Total In Words:</span> <span class=\"text-muted fst-capitalize\">{{ doc.in_words }}</span></p></td></tr>\n                    </tbody>\n                  </table>\n                </td>\n              </tr>\n\n              <!-- HSN SUMMARY TABLE -->\n              <tr>\n                <td colspan=\"10\" style=\"padding: 0 !important;border-top:0px;border:0px !important;\">\n                  <table class=\"table text-center\" style=\"width: 100%; border-collapse: collapse; border-top: 1px solid #b8b8b8;\">\n                    <thead class=\"bg-light fs-mini fw-bold\">\n                      <tr><td rowspan=\"2\" style=\"border-left: 0px !important;\" class=\"border\">HSN Code</td><td rowspan=\"2\" class=\"border\">Taxable Value</td><td rowspan=\"2\" class=\"border\">Qty.</td><td rowspan=\"2\" class=\"border\">UOM</td><td colspan=\"2\" class=\"border\">IGST</td><td colspan=\"2\" class=\"border\">CGST</td><td colspan=\"2\" class=\"border\">SGST</td><td colspan=\"2\" style=\"border-right: 0px !important;\" class=\"border\">CESS</td></tr>\n                      <tr><td class=\"border\">Rate</td><td class=\"border\">Amt</td><td class=\"border\">Rate</td><td class=\"border\">Amt</td><td class=\"border\">Rate</td><td class=\"border\">Amt</td><td class=\"border\">Rate</td><td class=\"border\" style=\"    border-right: 0px !important;\" >Amt</td></tr>\n                    </thead>\n                    <tbody class=\"fs-mini\">\n                      <!-- Simplified placeholder row -->\n                      <tr><td style=\"   