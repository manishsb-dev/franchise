 "html": "<!-- JINJA: FETCH DATA -->\n{% set trans_addr = frappe.db.get_value(\"Address\", {\"address_title\": doc.transporter}, \"*\", as_dict=1) %}\n{% set cons_addr = frappe.db.get_value(\"Address\", {\"address_title\": doc.consignor}, \"*\", as_dict=1) %}\n{% set comp_addr = frappe.db.get_value(\"Address\", {\"address_title\": doc.owner_site}, \"*\", as_dict=1) %}\n\n<style>\n    /*@page { */\n    /*    size: A4 portrait; */\n    /*    margin: 5mm; */\n    /*    margin-bottom: 15mm; */\n    /*}*/\n    \n    html, body { \n        font-family: \"Arial\", sans-serif; \n        font-size: 10px; \n        color: #000; \n        line-height: 1.3; \n        margin: 0; padding: 0; \n    }\n    \n    .compact-table td {\n        padding-top: 0px !important;\n        padding-bottom: 1px !important;\n        line-height: 1.1 !important; /* Line height kam kar di */\n        vertical-align: top;\n    }\n    \n    /* RESET TABLES */\n    table { width: 100%; border-collapse: collapse; border-spacing: 0; }\n    td { vertical-align: top; padding: 2px 4px; }\n    \n    /* UTILS */\n    .bold { font-weight: bold; }\n    .text-right { text-align: right; }\n    .text-center { text-align: center; }\n    .uppercase { text-transform: uppercase; }\n    \n    /* BORDERS */\n    .main-table { border-top: 1px solid #000; border-bottom: 1px solid #000; }\n    .row-separator { border-bottom: 1px solid #000; } /* Line between Transporter and Details */\n    .v-line { border-right: 1px solid #000; } /* The Single Vertical Line */\n    \n    .thick-y { border-top: 2px solid #000; border-bottom: 2px solid #000; }\n    .thin-b { border-bottom: 1px solid #000; }\n    \n    /* LABELS */\n    .lbl { font-weight: bold; width: 1px; white-space: nowrap; padding-right: 5px; }\n    \n    /* FOOTER FIX */\n    .footer-table { \n        position: fixed; \n        bottom: 0; \n        left: 0; \n        width: 100%; \n        border-top: 1px solid #000; \n        background: white;\n    }\n</style>\n\n<!-- 1. HEADER SECTION (Corrected Alignment) -->\n<table style=\"margin-bottom: 5px; width: 100%;\">\n    <tr>\n        <!-- LEFT: Company Details -->\n        <td width=\"55%\" style=\"vertical-align: top; padding-right: 10px;\">\n            <!-- Company Name -->\n            <span style=\"font-size: 14px; font-weight: bold; display: block; margin-bottom: 2px;\">\n                {{ doc.owner_site or \"LAKHMI CHAND TEJOO MAL (Unit-1)\" }}\n            </span>\n            \n            <!-- Address -->\n            <span style=\"font-size: 10px; line-height: 1.3;\">\n                {% if comp_addr %}\n                    {{ comp_addr.address_line1 }}<br>\n                    {{ comp_addr.city }} - {{ comp_addr.pincode }}\n                {% else %}\n                    692, BAGH DEEWAR, STATION ROAD, CHANDNI CHOWK,<br>\n                    DELHI - 110006\n                {% endif %}\n            </span>\n            <br>\n            <span style=\"font-size: 10px;\">41300000.00</span>\n        </td>\n\n        <!-- RIGHT: LR & Logistic Details -->\n        <td width=\"45%\" align=\"right\" style=\"vertical-align: top;\">\n            <!-- \"LR\" Label at the very top right -->\n            <table style=\"width: 100%;\">\n                <tr>\n                    <td align=\"right\" style=\"font-weight: bold; font-size: 12px; padding: 0;\">LR</td>\n                </tr>\n            </table>\n\n            <!-- SPACER to create the gap between LR and Logistic No (Matches PDF) -->\n            <table style=\"width: 100%; height: 25px;\">\n                <tr><td>&nbsp;</td></tr>\n            </table>\n\n            <!-- Logistic Entry Details Table (Aligned Right) -->\n            <table style=\"width: auto; float: right; border-collapse: collapse;\">\n                <tr>\n                    <td class=\"bold text-right\" style=\"white-space: nowrap; padding: 1px 5px;\">Logistic Entry No. :</td>\n                    <td class=\"bold text-right\" style=\"white-space: nowrap; padding: 1px;\">{{ doc.entry_no or \"62798\" }}</td>\n                </tr>\n                <tr>\n                    <td class=\"bold text-right\" style=\"white-space: nowrap; padding: 1px 5px;\">Logistic Entry Date :</td>\n                    <td class=\"bold text-right\" style=\"white-space: nowrap; padding: 1px;\">{{ doc.get_formatted(\"date\") or \"02-12-2025\" }}</td>\n                </tr>\n            </table>\n        </td>\n    </tr>\n</table>\n\n<table class=\"main-table\">\n    \n    <!-- ROW A: TRANSPORTER & CONSIGNOR -->\n    <tr class=\"row-separator\">\n        <!-- LEFT: Transporter -->\n        <td width=\"50%\" class=\"v-line\" style=\"padding: 3px;\">\n            <!-- Added class 'compact-table' here -->\n            <table class=\"compact-table\">\n                <tr><td class=\"lbl\">Transporter</td><td>: <span class=\"uppercase\">{{ doc.transporter or \"MEHTA TRANS LOGISTICS\" }}</span></td></tr>\n                <tr><td class=\"lbl\">Transporter Address</td><td>: {{ trans_addr.address_line1 if trans_addr else \"CG-192, SANJAY GANDHI TRANSPORT NAGAR\" }}</td></tr>\n                <tr><td class=\"lbl\">Transporter GSTIN</td><td>: {{ trans_addr.tax_id or trans_addr.gstin or \"07AAJCM9042L2ZZ\" if trans_addr else \"\" }}</td></tr>\n                <tr><td class=\"lbl\">Transporter State</td><td>: {{ trans_addr.state if trans_addr else \"Delhi [07]\" }}</td></tr>\n            </table>\n        </td>\n        <!-- RIGHT: Consignor -->\n        <td width=\"50%\" style=\"padding: 3px;\">\n            <!-- Added class 'compact-table' here -->\n            <table class=\"compact-table\">\n                <tr><td class=\"lbl\">Consignor</td><td>: <span class=\"uppercase\">{{ doc.consignor or \"SHREE JI CREATION\" }}</span></td></tr>\n                <tr><td class=\"lbl\">Consignor Address</td><td>: {{ cons_addr.address_line1 if cons_addr else \"718 GROUND NTM MARKET RING ROAD\" }}</td></tr>\n                <tr><td class=\"lbl\">Consignor GSTIN</td><td>: {{ cons_addr.tax_id or cons_addr.gstin or \"24AEFFS7566A1Z2\" if cons_addr else \"\" }}</td></tr>\n                <tr><td class=\"lbl\">Consignor State</td><td>: {{ cons_addr.state if cons_addr else \"Gujarat [24]\" }}</td></tr>\n            </table>\n        </td>\n    </tr>\n\n    <!-- ROW B: LR / Invoice -->\n    <tr>\n        <td class=\"v-line\" style=\"padding: 1px 3px;\">\n            <table class=\"compact-table\" width=\"100%\">\n                <tr>\n                    <td><span class=\"lbl\">LR Doc No.</span> : {{ doc.lr_document_no or \"60022534\" }}</td>\n                    <td align=\"right\"><b>Date :</b> {{ doc.get_formatted(\"date\") or \"02-12-2025\" }}</td>\n                </tr>\n            </table>\n        </td>\n        <td style=\"padding: 1px 3px;\">\n            <table class=\"compact-table\" width=\"100%\">\n                <tr>\n                    <td><span class=\"lbl\">Invoice No.</span> : {{ doc.invoice_no or \"642012803811\" }}</td>\n                    <td align=\"right\"><b>Date :</b> {{ doc.get_formatted(\"date\") or \"02-12-2025\" }}</td>\n                </tr>\n            </table>\n        </td>\n    </tr>\n\n    <!-- ROW C: Station From / Logistic Type -->\n    <tr>\n        <td class=\"v-line\" style=\"padding: 1px 3px;\">\n            <table class=\"compact-table\" width=\"100%\">\n                <tr>\n                    <td><span class=\"lbl\">Station From</span> : <span class=\"uppercase\">{{ doc.station_from or \"SURAT\" }}</span></td>\n                </tr>\n            </table>\n        </td>\n        <td style=\"padding: 1px 3px;\">\n            <table class=\"compact-table\" width=\"100%\">\n                <tr>\n                    <td><span class=\"lbl\">Logistic Type</span> : {{ doc.type or \"Goods Receive Challan\" }}</td>\n                </tr>\n            </table>\n        </td>\n    </tr>\n\n    <!-- ROW D: Station To / Qty -->\n    <tr>\n        <td class=\"v-line\" style=\"padding: 1px 3px;\">\n            <table class=\"compact-table\" width=\"100%\">\n                <tr>\n                    <td><span class=\"lbl\">Station To</span> : <span class=\"uppercase\">{{ doc.station_to or \"DELHI\" }}</span></td>\n                </tr>\n            </table>\n        </td>\n        <td style=\"padding: 1px 3px;\">\n            <table class=\"compact-table\" width=\"100%\">\n                <tr>\n                    <td><span class=\"lbl\">Logistic Qty.</span> : {{ doc.lr_quantity or \"1.000\" }}</td>\n                    <td align=\"right\"><b>Pend. Lgst. Qty. :</b> {{ doc.pending_qty or \"0.000\" }}</td>\n                </tr>\n            </table>\n        </td>\n    </tr>\n\n    <!-- ROW E: Mode / To Pay -->\n    <tr>\n        <td class=\"v-line\" style=\"padding: 1px 3px;\">\n            <table class=\"compact-table\" width=\"100%\">\n                <tr>\n                    <td style=\"white-space: nowrap; width: 1%; padding-right: 15px;\">\n                        <span class=\"lbl\" style=\"width: auto;\">Mode of Transport</span> : {{ doc.mode or \"Road\" }}\n                    </td>\n                    \n                    <!-- Vehicle No (Right Side of the same block) -->\n                    <td align=\"right\" style=\"white-space: nowrap;\">\n                        <b>Vehicle No. :</b> {{ doc.vehicle_no or \"\" }}\n                    </td>\n                </tr>\n            </table>\n        </td>\n        <td style=\"padding: 1px 3px;\">\n            <table class=\"compact-table\" width=\"100%\">\n                <tr>\n                    <td><span class=\"lbl\">To Pay</span> : {{ doc.to_pay or \"Yes\" }}</td>\n                </tr>\n            </table>\n        </td>\n    </tr>\n\n    <!-- ROW F: Declaration -->\n    <tr>\n        <td class=\"v-line\" style=\"padding: 1px 3px;\">\n            <table class=\"compact-table\" width=\"100%\">\n                <tr>\n                    <td><span class=\"lbl\">Declaration Amt.</span> : {{ doc.declaration_amount or \"\" }}</td>\n                </tr>\n            </table>\n        </td>\n        <td style=\"padding: 1px 3px;\">\n            <!-- Empty Right Cell -->\n        </td>\n    </tr>\n</table>\n\n<!-- 3. CHARGES TABLE -->\n<table style=\"margin-top: 15px;\">\n    <!-- Thick Borders Header -->\n    <tr class=\"bold thick-y\" style=\"background-color: #f0f0f0;\">\n        <td width=\"15%\">Actual Weight</td>\n        <td width=\"15%\" class=\"text-center\">Charged Weight</td>\n        <td width=\"10%\" class=\"text-center\">Rate</td>\n        <td width=\"20%\" class=\"text-right\">Freight</td>\n        <td width=\"20%\" class=\"text-right\">Others</td>\n        <td width=\"20%\" class=\"text-right\">Total Freight</td>\n    </tr>\n    <!-- Data -->\n    <tr class=\"thin-b\">\n        <td>{{ doc.actual_weight or \"365.00\" }}</td>\n        <td class=\"text-center\">{{ doc.charged_weight or \"365.00\" }}</td>\n        <td class=\"text-center\">{{ doc.rate or \"\" }}</td>\n        <td class=\"text-right\">{{ doc.freight or \"\" }}</td>\n        <td class=\"text-right\">{{ doc.others or \"365.00\" }}</td>\n        <td class=\"text-right bold\">{{ doc.total_amount or \"365.00\" }}</td>\n    </tr>\n</table>\n\n<!-- 4. GATE ENTRY DETAILS -->\n<table style=\"margin-top: 15px;\">\n    <tr>\n        <td style=\"background-color: #e0e0e0; font-weight: bold; width: 450px; border: 1px solid #fff;\">Gate Entry Details</td>\n        <td></td>\n    </tr>\n</table>\n\n<table style=\"margin-top: 2px;\">\n    <!-- Header with Borders -->\n    <tr class=\"bold thick-y\" style=\"background-color: #f0f0f0;\">\n        <td>Gate Entry No.</td>\n        <td>Gate Entry Date</td>\n        <td>Document No.</td>\n        <td>Document Date</td>\n        <td class=\"text-right\">Gate In Qty.</td>\n        <td class=\"text-right\">Receive Qty.</td>\n    </tr>\n    <!-- Row -->\n    <tr>\n        <td>{{ doc.gate_entry or \"62514\" }}</td>\n        <td>{{ doc.gate_entry_date or \"02-12-2025\" }}</td>\n        <td>{{ doc.lr_document_no or \"60022534\" }}</td>\n        <td></td>\n        <td class=\"text-right\">{{ doc.lr_quantity or \"1.000\" }}</td>\n        <td class=\"text-right\">{{ doc.received_qty or \"1.000\" }}</td>\n    </tr>\n    <!-- Total Row -->\n    <tr class=\"bold thin-b\" style=\"border-top: 1px solid #ccc; background-color: #f0f0f0;\">\n        <td colspan=\"4\" class=\"text-right\">Total :</td>\n        <td class=\"text-right\">{{ doc.lr_quantity or \"1.000\" }}</td>\n        <td class=\"text-right\">{{ doc.received_qty or \"1.000\" }}</td>\n    </tr>\n</table>\n\n<!-- 5. SIGNATURES -->\n<br>\n<table width=\"100%\" style=\"margin-top: 5px;\">\n    <tr>\n        <td align=\"right\" class=\"bold\" style=\"padding-bottom: 30px;\">\n            For {{ doc.owner_site or \"LAKHMI CHAND TEJOO MAL\" }}\n        </td>\n    </tr>\n</table>\n\n<table width=\"100%\" style=\"margin-top: 25px;\">\n    <tr>\n        <!-- Prepared By -->\n        <td width=\"33%\" valign=\"top\">\n            <table width=\"90%\" align=\"center\">\n                <tr><td  style=\"border-top: 1px solid #000; text-align: center;\"><b>Prepared By</b></td></tr>\n                <tr><td class=\"text-center\" style=\"font-size: 9px;\">TR-01-LT [{{ doc.owner }}] [{{ doc.get_formatted(\"creation\") }}]</td></tr>\n            </table>\n        </td>\n        \n        <!-- Checked By -->\n        <td width=\"33%\" valign=\"top\">\n            <table width=\"90%\" align=\"center\">\n                <tr><td  style=\"border-top: 1px solid #000; text-align: center;\"><b>Checked By</b></td></tr>\n            </table>\n        </td>\n        \n        <!-- Passed By -->\n        <td width=\"33%\" valign=\"top\">\n            <table width=\"90%\" align=\"right\">\n                <tr><td  style=\"border-top: 1px solid #000; text-align: center ;\"><b>Passed By</b></td></tr>\n            </table>\n        </td>\n    </tr>\n</table>\n\n<!-- 6. FIXED FOOTER -->\n<table class=\"footer-table\">\n    <tr>\n        <td align=\"left\">Print Date: {{ frappe.utils.today() }}</td>\n        \n        <td align=\"right\">Page No.: 1</td>\n    </tr>\n</table>",
